{
  "python.linting.pylintArgs": ["--disable=W0104"],
  "files.autoSave": "onFocusChange",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.wordWrap": "on",
  "javascript.preferences.quoteStyle": "double",
  "liveServer.settings.donotShowInfoMsg": true,
  "explorer.confirmDelete": false,
  "workbench.preferredDarkColorTheme": "Monokai Pro",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "python.testing.pytestEnabled": true,
  "explorer.confirmDragAndDrop": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.minimap.enabled": false,
  "window.autoDetectColorScheme": true,
  "javascript.format.semicolons": "remove",
  "[javascript]": {
    "diffEditor.codeLens": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "git.enableSmartCommit": true,
  "files.autoSaveDelay": 20000,
  "todo-tree.general.tags": [
    "BUG",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]",
    "@todo",
    "DEBUG"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|/\\*|^\\s*\\*)\\s*($TAGS)",
  "todo-tree.regex.regexCaseSensitive": false,
  "git.pruneOnFetch": true,
  "git.fetchOnPull": true,
  "todo-tree.filtering.excludeGlobs": [
    "node_modules/**",
    ".next/**",
    ".gitignore",
    ".gitattributes"
  ],
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false,
    "yaml": true,
    "python": true
  },
  "remote.SSH.defaultExtensions": ["gitpod.gitpod-remote-ssh"],
  "eslint.alwaysShowStatus": true,
  "eslint.run": "onSave",
  "prettier.singleQuote": true,
  "prettier.jsxSingleQuote": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.autofetch": true,
  "git.autofetchPeriod": 180,
  "git.autorefresh": true,
  "remoteHub.commitDirectlyWarning": "off",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "workbench.colorTheme": "Monokai Pro",
  "workbench.iconTheme": "Monokai Pro Icons",
  "jupyter.widgetScriptSources": ["jsdelivr.com", "unpkg.com"],
  "notebook.formatOnSave.enabled": true,
  "editor.fontSize": 13,
  "editor.guides.bracketPairs": "active",
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingQuotes": "never",
  "editor.linkedEditing": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "githubPullRequests.createOnPublishBranch": "never",
  "editor.formatOnSave": true,
  "todo-tree.general.statusBar": "total",
  "todo-tree.highlights.backgroundColourScheme": [
    "red",
    "orange",
    "yellow",
    "green",
    "blue",
    "indigo",
    "violet",
    "white"
  ],
  "todo-tree.highlights.foregroundColourScheme": [
    "white",
    "black",
    "green",
    "green",
    "white",
    "white",
    "black"
  ],
  "todo-tree.filtering.ignoreGitSubmodules": true,
  "todo-tree.tree.showCurrentScanMode": false,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontFamily": "'Fira Code', monospace",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.letterSpacing": 0.5,
  "terminal.integrated.lineHeight": 1.1,
  "terminal.integrated.fontWeight": "400",
  "terminal.integrated.fontWeightBold": "700",
  "terminal.integrated.cursorWidth": 1,
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "terminal.integrated.minimumContrastRatio": 4.5,
  "files.exclude": {
    "**/.pytest_cache": true,
    "**/__pycache__": true,
    "**/LICENSE": true,
    "**/.gitattributes": true
    // "**/.gitmodules": true
    // "**/.gitignore": true,
    // "**/.eslintignore": true,
    // "**/.eslintrc.js": true,
    // "**/.gitattributes": true,
    // "**/.next": true,
    // "**/.nvmrc": false,
    // "**/.prettierignore": true,
    // "**/.prettierrc": true,
    // "**/.solhint.json": true,
    // "**/.venv": false,
    // "**/.yarn": true,
    // "**/CONTRIBUTING.md": true,
    // "**/default.nix": true,
    // "**/jsconfig.json": true,
    // "**/next-env.d.ts": true,
    // "**/next.config.js": true,
    // "**/node_modules": true,
    // "**/package-lock.json": true,
    // "**/package.json": true,
    // "**/postcss.config.js": true,
    // "**/prettier.config.js": true,
    // "**/requirements.txt": true,
    // "**/tailwind.config.js": true,
    // "**/tsconfig.json": true,
    // "**/yarn-error.log": true,
    // "**/yarn.lock": true
  },
  "javascript.updateImportsOnFileMove.enabled": "never",
  "typescript.updateImportsOnFileMove.enabled": "never",
  "notebook.lineNumbers": "on",
  "workbench.editorAssociations": {
    "*.db": "sqlite-viewer.option"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "window.density.editorTabHeight": "compact",
  "python.analysis.diagnosticSeverityOverrides": {
    "reportUnusedVariable": "none",
    "reportUnusedExpression": "none"
  },
  "python.analysis.typeCheckingMode": "off",
  "git.openRepositoryInParentFolders": "always",
  "scm.alwaysShowRepositories": true,
  "application.shellEnvironmentResolutionTimeout": 30,
  "editor.accessibilitySupport": "off",
  "interactiveWindow.executeWithShiftEnter": true,
  "workbench.preferredLightColorTheme": "Monokai Pro Light (Filter Sun)",
  "explorer.confirmPasteNative": false,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  },
  "[solidity]": {
    "editor.defaultFormatter": "JuanBlanco.solidity"
  },
  "solidity.formatter": "forge",
  "workbench.statusBar.visible": false
}
