[submodule "subrepos/react.dev"]
    path = subrepos/react.dev
    url  = https://github.com/reactjs/react.dev.git
    branch = main         
[submodule "subrepos/expo"]
    path = subrepos/expo
    url  = https://github.com/expo/expo.git
    branch = main
[submodule "subrepos/next.js"]
    path = subrepos/next.js
    url  = https://github.com/vercel/next.js.git
    branch = canary
[submodule "subrepos/cloudflare-docs"]
    path = subrepos/cloudflare-docs
    url  = https://github.com/cloudflare/cloudflare-docs.git
    branch = production
[submodule "subrepos/three.js"]
    path = subrepos/three.js
    url  = https://github.com/mrdoob/three.js.git
    branch = dev
[submodule "subrepos/openai-cookbook"]
    path = subrepos/openai-cookbook
    url  = https://github.com/openai/openai-cookbook.git
    branch = main

# How to add a new submodule:
# git submodule add -b <branch> --depth 1 <repository-url> subrepos/<repo-name>
# Example:
# git submodule add -b main --depth 1 https://github.com/lodash/lodash.git subrepos/lodash
